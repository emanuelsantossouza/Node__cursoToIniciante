<!DOCTYPE html>
<html lang="en">
<%- include('./cadastro/header.ejs')%>


<body>
    <%- include('./cadastro/navbar.ejs') %>

    <div class="box">
        <form action="/users/cadastro" method="POST">
            <fieldset>
                <legend><b>Fórmulário de Clientes</b></legend>
                <br>
                <div class="inputBox">
                    <input type="text" name="nome" id="nome" class="inputUser" required>
                    <label for="nome" class="labelInput">First Name</label>
                </div>
                <br><br>
                <div class="inputBox">
                    <input type="tel" name="telefone" id="telefone" class="inputUser" required>
                    <label for="telefone" class="labelInput">Last Name</label>
                </div>
                <br><br>
                <div class="inputBox">
                    <input type="text" name="email" id="email" class="inputUser" required>
                    <label for="email" class="labelInput">Email</label>
                </div>
                <br><br>
              
                <div class="inputBox">
                    <input type="password" name="endereco" id="endereco" class="inputUser" required>
                    <label for="endereco" class="labelInput">Password</label>
                </div>
                <br><br>
                <input type="submit" name="submit" id="submit" >
            </fieldset>
        </form>
    </div>
</body>
</html>